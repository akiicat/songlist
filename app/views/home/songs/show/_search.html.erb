<tr class="tr-wrapper">
  <td><code class="singer"><%= song.singer.name_with_translation %></code></td>
  <td>
    <code>
      <%= song.title_with_translation %>
      <span class="description"><%= song.supplement %></span>
    </code>
  </td>
  <td>
    <span class="icon-wrapper right pointer" data-aria-label="複製歌名" data-tipsy-hint="已複製！">
      <%= octicon "clippy", class: "icon", "data-clippy": song.name_all, "data-clippy-duplicate": false %>
    </span>
    <span class="icon-wrapper right">
      <% unless song.lyric_url.blank? %>
        <%= link_to song.lyric_url, target: "_blank", "data-aria-label": "歌詞" do %>
          <%= svg "lyrics" %>
        <% end %>
      <% end %>
    </span>
    <span class="icon-wrapper right">
      <% unless song.video_url.blank? %>
        <%= link_to song.video_url, target: "_blank", "data-aria-label": "YouTube" do %>
          <%= svg "youtube" %>
        <% end %>
      <% end %>
    </span>
  </td>
</tr>

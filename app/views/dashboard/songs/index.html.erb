<header>
  <h1>
    <span id="songs_count"><%= @songs.count %></span>
    首
  </h1>
</header>
<table class="table table-striped">
  <col width="30%">
  <col width="50%">
  <thead>
    <tr>
      <th>歌手</th>
      <th>歌名</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody id="songs">
    <%= render partial: "dashboard/songs/show/search", collection: @songs, as: :song %>
  </tbody>
</table>

<div id="pagination">
  <%= will_paginate @songs %>
</div>

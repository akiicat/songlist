var rtn = "<%= j render(partial: 'dashboard/songs/show/search', collection: @songs, as: :song) %>"

<% if @songs.previous_page %>
  $('#songs').append(rtn);
<% else %>
  $('#songs').html(rtn);
<% end %>

$('#pagination').html("<%= j will_paginate(@songs) %>");
$("#songs_count").html("<%= @songs.count %>");
$(document).ready(clippy);

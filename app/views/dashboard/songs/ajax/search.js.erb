<% if @songs.previous_page %>
  $('#songs').append("<%= j render(partial: 'dashboard/songs/show/search', collection: @songs, as: :song) %>");
<% else %>
  $('#songs').html("<%= j render(partial: 'dashboard/songs/show/search', collection: @songs, as: :song) %>");
<% end %>

$('#pagination').html("<%= j will_paginate(@songs) %>");
$("#songs_count").html("<%= @songs.count %>");
$(document).ready(clippy);

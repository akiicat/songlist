<% if @songs.previous_page %>
  $('#songs').append("<%= j render('dashboard/songs/show/search') %>");
<% else %>
  $("#songs").html("<%= escape_javascript(render 'dashboard/songs/show/search' ) %>");
<% end %>

$('#pagination').html("<%= j will_paginate(@songs) %>");
$("#songs_count").html("<%= @songs.count %>");
$(document).ready(clippy);
